<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>

<StackPane prefWidth="255.0" styleClass="stack-pane" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <HBox prefHeight="0.0" prefWidth="200.0" spacing="20.0">
      <children>
         <Label id="context" fx:id="contextLabel" maxHeight="1.7976931348623157E308" styleClass="context_label" text="mods" HBox.hgrow="ALWAYS" />
        <TextField fx:id="commandTextField" onAction="#handleCommandEntered" promptText="Enter command here..." HBox.hgrow="ALWAYS" />
      </children>
      <StackPane.margin>
         <Insets left="20.0" />
      </StackPane.margin>
   </HBox>
</StackPane>
