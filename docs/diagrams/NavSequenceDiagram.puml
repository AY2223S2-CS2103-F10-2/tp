@startuml
!include style.puml

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":NavigationInjector" as NavigationInjector LOGIC_COLOR
participant ":TrackerParser" as AddressBookParser LOGIC_COLOR
participant ":AddCommandParser" as AddCommandParser LOGIC_COLOR
participant "d:AddVideoCommand" as AddVideoCommand LOGIC_COLOR
participant ":CommandResult" as CommandResult LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
end box

[-> LogicManager : execute("add Video 1")
activate LogicManager

LogicManager -> TrackerParser : parseCommand("delete 1")
activate TrackerParser

create AddCommandParser
TrackerParser -> AddCommandParser
activate AddCommandParser

AddCommandParser --> TrackerParser
deactivate AddCommandParser

TrackerParser -> AddCommandParser : parse("1")
activate AddCommandParser

create AddCommand
AddCommandParser -> AddCommand
activate AddCommand

AddCommand --> AddCommandParser : d
deactivate AddCommand

AddCommandParser --> TrackerParser : d
deactivate AddCommandParser
'Hidden arrow to position the destroy marker below the end of the activation bar.
AddCommandParser -[hidden]-> TrackerParser
destroy AddCommandParser

TrackerParser --> LogicManager : d
deactivate TrackerParser

LogicManager -> AddCommand : execute()
activate AddCommand

AddCommand -> Model : deletePerson(1)
activate Model

Model --> AddCommand
deactivate Model

create CommandResult
AddCommand -> CommandResult
activate CommandResult

CommandResult --> AddCommand
deactivate CommandResult

AddCommand --> LogicManager : result
deactivate AddCommand

[<--LogicManager
deactivate LogicManager
@enduml
